@article{
title = {Projetos orientados para o mercado: análise em um cluster de Tecnologia da Informação}
,author = {Lara, J. E. and  Locatelli, R. L. and  Machado, M. V. C. and  Sousa, C. V.}
,journal = {Revista Ibero-Americana de Estratégia}
,year = {2014}
